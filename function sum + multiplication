using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Globalization;
using System.Threading.Tasks;


namespace Csharp_education
{
    class Program
    {
        static int Sum(int a, int b)
        {
            int result = a + b;
            return result;
        }

        static int Umn(int a, int b, int c)
        {
            return a * b * c;
        }
        static void Main(string[] args)
        {
            int a, b, c;
            c = Sum(13, 21);
            b = Umn(51, 22, 1);
            Console.WriteLine(c);
            Console.WriteLine(b);
        }
    }
}
