using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Globalization;
using System.Threading.Tasks;


namespace Csharp_education
{
    class Program
    {
        static void Main(string[] args)
        {
            for (int i = 1; i < 5+1; i++)
            {
                Console.WriteLine("Цикл:" + i);
                for (int j = 1; j < 5+1; j++)
                {
                    Console.WriteLine(" Вложенный цикл " + j);
                }
            }
        }
    }
}
